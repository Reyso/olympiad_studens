   "source": [
    "df['cat_experiencia'] = 'Null'\n",
    "\n",
    "\n",
    "# \"Experiência de trabalho e requer treinamento\", - 1\n",
    "df.loc[(df['tem_experiencia_de_trabalho'] == 'S') & (df['requer_treinamento_de_trabalho'] == 'S'), 'cat_experiencia'] = 1\n",
    "# \"Experiência de trabalho e não requer treinamento\" - 2\n",
    "df.loc[(df['tem_experiencia_de_trabalho'] == 'S') & (df['requer_treinamento_de_trabalho'] == 'N'), 'cat_experiencia'] = 2\n",
    "# \"Sem experiência de trabalho e requer treinamento\" - 3\n",
    "df.loc[(df['tem_experiencia_de_trabalho'] == 'N') & (df['requer_treinamento_de_trabalho'] == 'S'), 'cat_experiencia'] = 3\n",
    "# \"Sem experiência de trabalho e não requer - 4\n",
    "df.loc[(df['tem_experiencia_de_trabalho'] == 'N') & (df['requer_treinamento_de_trabalho'] == 'N'), 'cat_experiencia'] = 4\n",
