    "# Plotar histograma dos salários\n",
    "salarios  = df['salario_prevalecente']\n",
    "\n",
    "plt.hist(salarios, bins=50, edgecolor='black')\n",
    "\n",
    "# Definir os intervalos de classificação\n",
    "percentil_25 = np.percentile(salarios, 25)\n",
    "percentil_75 = np.percentile(salarios, 75)\n",
    "median = np.percentile(salarios,50)\n",
    "\n",
    "# Adicionar linhas verticais para os intervalos de classificação\n",
    "plt.axvline(percentil_25, color='red', linestyle='--', label='Baixo')\n",
    "plt.axvline(median, color='gray', linestyle='--', label='Medio')\n",
    "plt.axvline(percentil_75, color='blue', linestyle='--', label='Alto')\n",
    "\n",
