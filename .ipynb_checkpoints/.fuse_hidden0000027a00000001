    "\n",
    "anos = df['ano_de_estabelecimento']\n",
    "\n",
    "# Definir os intervalos de classificação\n",
    "percentil_25 = np.percentile(anos, 25)\n",
    "percentil_75 = np.percentile(anos, 75)\n",
    "\n",
    "# Adicionar linhas verticais para os intervalos de classificação\n",
    "plt.axvline(percentil_25, color='red', linestyle='--', label='Antiga')\n",
    "plt.axvline(percentil_75, color='blue', linestyle='--', label='Nova')\n",
